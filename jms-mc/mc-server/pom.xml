			<groupId>commons-net</groupId>
			<artifactId>commons-net</artifactId>
			<version>${commons-net.version}</version>
		</dependency>
		<dependency>
			<groupId>it</groupId>
			<artifactId>logutil</artifactId>
			<version>${it.logutil.version}</version>
		</dependency>
		<dependency>
			<groupId>it</groupId>
			<artifactId>enumeration</artifactId>
			<version>${it.enumeration.version}</version>
		</dependency>
