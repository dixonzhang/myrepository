<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>

	<groupId>com.mama100.monitorcenter</groupId>
	<artifactId>monitor-center</artifactId>
	<version>0.0.1-SNAPSHOT</version>
	<name>monitor-center</name>
	<description>监控中心</description>
	<packaging>war</packaging>

	<properties>
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
		<!-- 依赖版本号 -->
		<junit.version>4.11</junit.version>
		<javax.jms.version>2.0</javax.jms.version>
		<org.glassfish.mq.version>5.0</org.glassfish.mq.version>
		<google.gson.version>2.2.4</google.gson.version>
		<it.enumeration.version>1.0.3</it.enumeration.version>
		<it.logutil.version>1.0.1</it.logutil.version>
		<org.springframework.version>3.2.8.RELEASE</org.springframework.version>
		<org.spring-support.version>2.0.8</org.spring-support.version>
		<org.spring-jms.version>4.0.5.RELEASE</org.spring-jms.version>
		<org.spring-webmvc.version>3.2.8.RELEASE</org.spring-webmvc.version>
		<org.quartz.version>2.2.1</org.quartz.version>
		<httpclient.version>4.3.1</httpclient.version>
		<commons-lang3.version>3.3.2</commons-lang3.version>
		<commons-net.version>3.3</commons-net.version>
	</properties>

